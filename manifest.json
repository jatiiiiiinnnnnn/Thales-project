{
    "manifest_version": 3,
    "name": "AI Security Guardian",
    "version": "1.0",
    "description": "AI-powered security extension for detecting malicious URLs and phishing attempts",
    "permissions": [
      "activeTab",
      "storage",
      "webNavigation",
      "notifications",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }],
    "host_permissions": [
      "<all_urls>"
    ]
  }
  